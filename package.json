  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "prettier",
      "react",
      "react-hooks"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "es6": true,
      "jest": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "eqeqeq": [
        "error",
        "always"
      ],
      "import/order": "error",
      "no-console": "error",
      "no-empty": "off",
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "all"
        }
      ],
      "prettier/prettier": "error",
      "react/display-name": "off",
      "react/prop-types": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  }
